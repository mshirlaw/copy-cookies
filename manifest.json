{
  "manifest_version": 3,
  "name": "Copy Cookies",
  "version": "1.0",
  "description": "Copy cookies from any domain to localhost",
  "permissions": [
    "cookies",
    "activeTab",
    "tabs"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Copy Cookies"
  },
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  }
}